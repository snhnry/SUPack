{
    "multipart": [
        {   "when": { "north": false },
            "apply": { "model": "block/pipe_section" }
        },
        {   "when": { "east": false },
            "apply": { "model": "block/pipe_section", "y": 90}
        },
        {   "when": { "south": false },
            "apply": { "model": "block/pipe_section", "y": 180}
        },
        {   "when": { "west": false },
            "apply": { "model": "block/pipe_section", "y": 270}
        },
        {   "when": { "up": false },
            "apply": { "model": "block/pipe_section", "x": 270}
        },
        {   "when": { "down": false },
            "apply": { "model": "block/pipe_section", "x": 90}
        },
        {   "when": { "north": true },
            "apply": [
                { "model": "block/pipe_middle" }
            ]
        },
        {   "when": { "east": true },
            "apply": [
                { "model": "block/pipe_middle", "y": 90}
            ]
        },
        {   "when": { "south": true },
            "apply": [
                { "model": "block/pipe_middle", "y": 180}
            ]
        },
        {   "when": { "west": true },
            "apply": [
                { "model": "block/pipe_middle", "y": 270}
            ]
        },
        {   "when": { "up": true },
            "apply": [
                { "model": "block/pipe_middle", "x": 270}
            ]
        },
        {   "when": { "down": true },
            "apply": [
                { "model": "block/pipe_middle", "x": 90}
            ]
        },
		{   "when": { "north": true, "east": true, "south": false, "west": true, "up": true, "down": true },          
            "apply": [
			    { "model": "block/pipe_end" }
			]
        },
		{   "when": { "north": false, "east": true, "south": true, "west": true, "up": true, "down": true },          
            "apply": [
			    { "model": "block/pipe_end", "y": 180 }
			]
        },
		{   "when": { "north": true, "east": false, "south": true, "west": true, "up": true, "down": true },          
            "apply": [
			    { "model": "block/pipe_end", "y": 270 }
			]
        },
		{   "when": { "north": true, "east": true, "south": true, "west": false, "up": true, "down": true },          
            "apply": [
			    { "model": "block/pipe_end", "y": 90 }
			]
        },
		{   "when": { "north": true, "east": true, "south": true, "west": true, "up": false, "down": true },          
            "apply": [
			    { "model": "block/pipe_end", "x": 90 }
			]
        },
		{   "when": { "north": true, "east": true, "south": true, "west": true, "up": true, "down": false },          
            "apply": [
			    { "model": "block/pipe_end", "x": 270 }
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "x": 90 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "x": 180 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "x": 270 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "y": 90 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "y": 180 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end", "y": 270 }			
			]
        },
		{   "when": {"down": false, "east": false, "north": false, "south": false, "up": false, "west": false },          
            "apply": [
			    { "model": "block/pipe_end"}			
			]
        }
    ]
}
